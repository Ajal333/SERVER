<h1>Workshop Manager</h1>


<form action="ws" method="POST">
    <div>
        <label for="name">Name</label>
        <input type="text" name="name" id="event" value="<%= (ws==='')?'':ws.name %>" required>
    </div>
    <div>
        <label for="branch">Branch</label>
        <input type="text" name="branch" id="branch" value="<%= (ws==='')?'':ws.branch %>" required>
    </div>
    <div>
        <label for="price">Rate</label>
        <input type="text" name="price" id="price" value="<%= (ws==='')?'':ws.price %>" required>
    </div>
    <div>
        <label for="date">Date</label>
        <input type="text" name="date" id="date" value="<%= (ws==='')?'':ws.date %>" required>
    </div>
    <div>
        <label for="content">Content</label>
        <textarea name="content" cols="50" rows="5" value="<%= (ws==='')?'':ws.content %>"></textarea>
    </div>
    <div>
        <label for="label">Label</label>
        <input type="text" name="label" id="label" value="<%= (ws==='')?'':ws.label %>" required>
    </div>
    <div>
        <p>Contacts:</p>
        <label for="contact1">Contact-1 name : </label>
        <input type="text" name="contactname1" id="" value="<%= (ws==='')?'':ws.contact[0].name %>">
        <label for="contact1">Contact-1 phone</label>
        <input type="text" name="contactphone1" id="" value="<%= (ws==='')?'':ws.contact[0].phone %>">
        <div></div>
        <label for="contact2">Contact-2 name : </label>
        <input type="text" name="contactname2" value="<%= (ws==='')?'':ws.contact[1].name %>">
        <label for="contact2">Contact-2 phone</label>
        <input type="text" name="contactphone2" value="<%= (ws==='')?'':ws.contact[1].phone %>">
    </div>
    <div>
        <label for="message">Add Message : </label>
        <input type="text" name="message" value="<%= (ws==='')?'':ws.message %>">
    </div>
    <div>
        <label for="details">Details : </label>
        <input type="text" name="details" value="<%= (ws==='')?'':ws.details %>">
    </div>
    <div>
        <label for="pdgUrl">PDF url : </label>
        <input type="text" name="pdf" value="<%= (ws==='')?'':ws.pdfUrl %>">
    </div>
    <div>
        <label for="url">Payment url</label>
        <input type="text" name="url" value="<%= (ws==='')?'':ws.url %>" required>
    </div>
    <div>
        <button>Submit</button>
    </div>

</form>